<div class="card">
    <div class="card-header text-center" style="font-weight:bold">
        <label style="font-weight:bold;">Simulador de un Sistema de Votación</label>
    </div>

    <div class="card-body">

        <div class="form-group">
            <label>Introduzca la cantidad de Miembros del Partido Comunista:</label>
            <input type="number" @bind="MiembrosC" class="form-control" />
        </div>

        <div class="form-group">
            <label>Introduzca la cantidad de Miembros del Partido Popular:</label>
            <input type="number" @bind="MiembrosP" class="form-control" />
        </div>

        <div class="form-group">
            <label>Introduzca la cantidad de Miembros del Partido Social:</label>
            <input type="number" @bind="MiembrosS" class="form-control" />
        </div>

        <div class="form-group">
            <label>Introduzca la cantidad de Miembros del Partido Republicano:</label>
            <input type="number" @bind="MiembrosR" class="form-control" />
        </div>

        <div class="form-group">
            <label>Introduzca la cantidad de Votantes:</label>
            <input type="number" @bind="Votantes" class="form-control" />
        </div>

    </div>

    @if (MiembrosC > 0 && MiembrosP > 0 && MiembrosS > 0 && MiembrosR > 0 && Votantes > 0)
    {
        <div class="card-footer">
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#PresidenteModal">Empezar proceso de votación </button>
        </div>
    }
    else
    {
        <div class="card-footer">
            <button type="button" class="btn btn-secondary" data-toggle="modal" disabled>Empezar proceso de votación </button>
        </div>
    }

</div>



